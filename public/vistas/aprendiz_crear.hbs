<body class="vertical  light  ">
  <div class="wrapper">
    <nav class="topnav navbar navbar-light">
      <button type="button" class="navbar-toggler text-muted mt-2 p-0 mr-3 collapseSidebar">
        <i class="fe fe-menu navbar-toggler-icon"></i>
      </button>

      <ul class="nav">
        <li class="nav-item">
          <a class="nav-link text-muted my-2" href="#" id="modeSwitcher" data-mode="dark">
            <i class="fe fe-sun fe-24"></i>
          </a>
        </li>

        <li class="nav-item nav-notif">
          <a class="nav-link text-muted my-2" href="./#" data-toggle="modal" data-target=".modal-notif">
            <span class="fe fe-bell fe-24"></span>
            <span class="dot dot-md bg-success"></span>
          </a>
        </li>
        <li class="nav-item dropdown">
          <a class="nav-link dropdown-toggle text-muted pr-0" href="#" id="navbarDropdownMenuLink" role="button"
            data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            <span class="avatar avatar-sm mt-2">
              <img src="./assets/avatars/face-1.jpg" alt="..." class="avatar-img rounded-circle">
            </span>
          </a>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
            <a class="dropdown-item" href="#">Perfil</a>
          </div>
        </li>
      </ul>
    </nav>





    <main role="main" class="main-content">
      <div class="container-fluid">
        <div class="row justify-content-center">
          <div class="col-11">
            <h2 class="page-title">Crear Residentes</h2>
            <div class="card shadow mb-4">
              <div class="form-group mb-3">
                <label for="example-password">Documento</label>
                <input type="text" id="documento" class="form-control" onchange="" required>
                <div id="FB_documento"></div>
              </div>
              <div class="form-group mb-3">
                <label for="example-password">Nombre</label>
                <input type="text" id="nombre" class="form-control" onchange="" required>
                <div id="FB_nombre"></div>
              </div>


              <div id="div_area" class="form-group mb-3">
                <label for="example-password">Nota 1</label>
                <input type="number" id="nota_1" class="form-control" onchange="" required>
                <div id="FB_nota_1"></div>
              </div>

              <div id="div_area" class="form-group mb-3">
                <label for="example-password">Nota 2</label>
                <input type="number" id="nota_2" class="form-control" onchange="" required>
                <div id="FB_nota_1"></div>
              </div>

              <div id="div_area" class="form-group mb-3">
                <label for="example-password">Nota 3</label>
                <input type="number" id="nota_3" class="form-control" onchange="" required>
                <div id="FB_nota_1"></div>
              </div>

              <div class="card-body">
                <button id="boton_crear" type="button" class="btn mb-2 btn-primary btn-block"><span
                    class="fe mr-2"></span>{{boton_crear}} </button>
                <a href="{{datagrid}}"><button id="regresar" type="button" class="btn mb-2 btn-light btn-block"><span
                      class="fe mr-2"></span>Regresar</button></a>
              </div>

            </div>
          </div>
        </div>
      </div> <!-- / .card -->
    </main> <!-- main -->
  </div> <!-- .wrapper -->

  <script src="../js/funciones.js"></script>

  <script src="../js/validaciones.js"></script>

